var fact = (n) => {
    if (n < 2) {
        return 1;
    } else {
        return n * fact(n - 1);
    }
};


var fibonacci = function (n) {
    return (
    (1 / Math.sqrt(5)) *
    (Math.pow((1 + Math.sqrt(5)) / 2, i) - Math.pow((1 - Math.sqrt(5)) / 2, i))
  );
}

var gcd = (a, b) => {
    while (b) {
        var t = b;
        b = a % b;
        a = t;
    }
    return a
};
